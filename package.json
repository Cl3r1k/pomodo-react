{
  "name": "pomodo-react",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "husky install",
    "lint-staged": "lint-staged",
    "server": "nodemon server/index.js",
    "client": "craco start",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "craco build",
    "test": "craco test",
    "lint-code": "eslint --debug ./",
    "lint-styles": "stylelint ./",
    "lint": "npm run lint-code && npm run lint-styles",
    "lint-code:fix": "eslint --fix ./",
    "lint-styles:fix": "stylelint --fix ./",
    "lint:fix": "npm run lint-code:fix && npm run lint-styles:fix",
    "prettier": "prettier --write ./",
    "update-build": "node --experimental-specifier-resolution=node --loader ts-node/esm ./generate.build.ts",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "craco-alias": "^2.1.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
