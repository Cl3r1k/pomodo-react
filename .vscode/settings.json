{
  "eslint.validate": ["javascript", "typescript", "html"],

  "eslint.options": {
    "extentions": [".js", ".ts", ".html"]
  },

  "scss.validate": false,
  "css.validate": false,
  "less.validate": false,

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  }
}
